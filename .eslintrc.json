{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-console": "warn",
    "linebreak-style": "off",
    "no-unused-vars": "warn",
    "quote-props": ["error", "consistent"],
    "func-names": "off",
    "no-underscore-dangle": "off"
  }
}
